        <rdfs:domain rdf:resource="fhkb-a2#Aboutness2"/>
        <rdfs:range rdf:resource="fhkb-a2#TimeIntervalRegion"/>
        <rdfs:comment>Property introduced to reify the ternary relation &apos;aboutness2&apos;</rdfs:comment>
    </owl:ObjectProperty>
    


