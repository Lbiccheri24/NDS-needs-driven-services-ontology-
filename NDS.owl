        <rdfs:domain rdf:resource="fhkb-a2#Description"/>
        <rdfs:range rdf:resource="fhkb-a2#Concept"/>
        <rdfs:comment>The relation between descriptions and concepts that are defined by such descriptions</rdfs:comment>
    </owl:ObjectProperty>
    


