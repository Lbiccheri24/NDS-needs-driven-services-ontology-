    <!-- fhkb-a2#pc_arg3_time -->

    <owl:ObjectProperty rdf:about="fhkb-a2#pc_arg3_time">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="fhkb-a2#Participation"/>
